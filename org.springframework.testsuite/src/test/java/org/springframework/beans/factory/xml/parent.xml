<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="inheritedTestBean" class="common.beans.core.TestBean" abstract="true">
		<property name="name"><value>parent</value></property>
		<property name="age"><value>1</value></property>
	</bean>

	<bean id="inheritedTestBeanWithoutClass" abstract="true">
		<property name="name"><value>parent</value></property>
		<property name="age"><value>1</value></property>
	</bean>

	<bean id="inheritedTestBeanPrototype" class="common.beans.core.TestBean" scope="prototype">
		<property name="name"><value>parent</value></property>
		<property name="age"><value>2</value></property>
	</bean>

	<bean id="inheritedTestBeanSingleton" class="common.beans.core.TestBean"/>

</beans>
